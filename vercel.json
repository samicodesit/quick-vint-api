{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,

  "builds": [
    {
      "src": "api/generate.ts",
      "use": "@vercel/node",
      "config": { "zeroConfig": true }
    }
  ],

  "routes": [
    {
      "src": "/api/generate",
      "dest": "/api/generate.js"
    },
    {
      "src": "/api/auth/magic-link",
      "dest": "/api/auth/magic-link.js"
    }
  ]
}
